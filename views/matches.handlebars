<body>
  <div id="title">
    <nav id="barbackground" class="navbar navbar-expand-lg">
      <a id="logo" class="navbar-brand" href="drafthome.html">Fit Finder</a>
      <button id="toggler" class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a id="signIn" class="nav-link" href="/login">Sign In</a>
          </li>
          <li class="nav-item">
            <a id="matches " class="nav-link" href="/matches">Matches</a>
          </li>
          <li class="nav-item">
            <a id="profPage" class="nav-link" href="/profile">Profile</a>
          </li>
        </ul>
      </div>
    </nav>
  </div>

  <div class="profileCardHolder">
    {{#each data}}
    <div class="card" id="profileCard" style="width: 18rem;">
      {{!-- <img class="img-circle" id="profilePic" src="images/lebron.png" alt="Card image cap"> --}}
      <div class="card-body">

        <h1 id="name">{{{this.name}}}</h1>
        <ul id="traits">
          <li>Email: {{{this.email}}}</li>
          <li>Activities: {{{activities}}}</li>
        </ul>

        <a href="/signUp"><button id="edit">Message</button></a>
      </div>
    </div>
    {{/each}}

