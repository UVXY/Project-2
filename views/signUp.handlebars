  <div class="container">
    <form id="signUp" name="signUp" method="post" action="/signUp">
      <div class="form-group">
        <label for="email">Email Address</label>
        <input placeholder="mike@here.com" class="form-control" class="text" name="email" type="email" />
      </div>
      <div class="form-group">
        <label for="firstname">Firstname</label>
        <input placeholder="Mike" class="form-control" name="firstname" type="text" />
      </div>
      <div class="form-group">
        <label for="lastname">Lastname</label>
        <input placeholder="Johnson" class="form-control" name="lastname" type="text" />
      </div>
      <div class="form-group">
        <label for="password">Password</label>
        <input placeholder="pa$$word" class="form-control" name="password" type="password" />
      </div>
  </div>

  <div class="parallax"></div>

  <h1 id="tellus">What Activities Interest You?</h1>

  <div class="container">
    <div class="form-group">
      <label for="basketball">Basketball (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="basketball" type="text" />
    </div>
    <div class="form-group">
      <label for="swimming">Swimming (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="swimming" type="text" />
    </div>
    <div class="form-group">
      <label for="gym">Gym (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="gym" type="text" />
    </div>
    <div class="form-group">
      <label for="running">Running (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="running" type="text" />
    </div>
    <div class="form-group">
      <label for="tennis">Tennis (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="tennis" type="text" />
    </div>
    <div class="form-group">
      <label for="soccer">Soccer (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="soccer" type="text" />
    </div>
    <div class="form-group">
      <label for="volleyball">Volleyball (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="volleyball" type="text" />
    </div>
    <div class="form-group">
      <label for="yoga">Yoga (Y/N)</label>
      <input placeholder="(Y/N)" class="form-control" name="yoga" type="text" />
    </div>
    <div class="form-group"></div>
    <label for="biking">Biking (Y/N)</label>
    <input placeholder="(Y/N)" class="form-control" name="biking" type="text" />
  </div>

  <input id="submit" class="btn" type="submit" value="Sign Up" />
  </form>
